<Ontology name="Android">
	<Primitives>
		<Primitive name="CPU_Usage" minE="0" maxE="100" />		
		<Primitive name="Garbage_Collections" minE="0" maxE="*" />
		
	</Primitives>
	
	<Events>
		<Event name="Picture_Taken" />
		<Event name="Keyboard_Opening" />
		<Event name="Keyboard_Closing" />
	</Events>

