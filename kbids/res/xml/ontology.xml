		<Context name="High_CPU_Usage">
			<Inductions>
				<Induction>
					<Primitive name="CPU_Usage" min="80" maxE="100" />

					<Ends relativeTo="end" gap="PT1M" />
				</Induction>			
				<Induction>
					<State name="CPU_Usage_Level" value="High" />

					<Ends relativeTo="end" gap="PT1M" />
				</Induction>
			</Inductions>
			
			<Destructions>
				<Primitive name="CPU_Usage" min="20" maxE="60" />

			</Destructions>
		</Context>
	</Contexts>
	
	<States>
		<State name="CPU_Usage_Level">
			<AbstractedFrom>
				<Primitive name="CPU_Usage" />
				<State name="blabla" />
			</AbstractedFrom>
			<NecessaryContexts>
				<Context name="Keyboard_Open" />
			</NecessaryContexts>
			<MappingFunction>
				<Value name="Low">
					<Primitive name="CPU_Usage" minE="0" maxE="30" />
					<State name="blabla" value="Low"/>
				</Value>
				<Value name="Normal">
					<Primitive name="CPU_Usage" minE="31" maxE="70" />
					<State name="blabla" value="Low"/>
				</Value>
				<Value name="High">
					<Primitive name="CPU_Usage" minE="71" maxE="100" />
					<State name="blabla" value="High"/>
				</Value>
			</MappingFunction>
			<InterpolationFunction>
				<Value name="Low" maxGap="PT1M" />
				<Value name="Normal" maxGap="P10D" />
				<Value name="High" maxGap="P10D" />
			</InterpolationFunction>
		</State>
	</States>
	
